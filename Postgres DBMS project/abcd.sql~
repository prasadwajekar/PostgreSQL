create Database DBM;
Create Table ABC(A_id int,A_name varchar(10),A_Address varchar(20),A_Mo int,Sex char(5));
create Table ABCD(D_id int,D_name varchar(10),D_Address varchar(30),D_Mo int,Sex char(10));
create Table ABCDE(E_id int,E_name varchar(10),E_Address varchar(30),E_Mo int,Sex char(10));
create Table ABCDF(F_id int,F_name varchar(10),F_Address varchar(30),F_Mo int,Sex char(10));
create Table ABCDG(G_id int,G_name varchar(10),G_Address varchar(30),G_Mo int,Sex char(10));
